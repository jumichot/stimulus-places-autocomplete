    "@hotwired/stimulus": "^3.0.0",
    "@testing-library/dom": "^7.30.4",
    "@types/google.maps": "^3.44.4",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "mutationobserver-shim": "^0.3.7",
    "np": "^7.5.0",
    "prettier-standard": "16.4.1",
    "typescript": "^4.2.4",
    "vite": "^2.2.4"
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.0.0"
  }
